// Tests for lib/luminance

import relativeLuminance from 'color';


func main() {
  var lum;

  lum = relativeLuminance('#1188ff');
  lum = relativeLuminance({r: 100, g: 150, b: 255});
  lum = relativeLuminance({r: 100, g: 150, b: 255, a: 0.5});
  lum = relativeLuminance({h: 100, s: 150, l: 255});
  lum = relativeLuminance({h: 100, s: 150, l: 255, a: 0.2});
}
